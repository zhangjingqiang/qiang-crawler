<% @doc.xpath("//li[@data-item-type='tweet']").each { |tweet| %>
	<div class="alert alert-info">
    <p><%= Time.at(tweet.xpath(".//a[@class='tweet-timestamp js-permalink js-nav js-tooltip']/span").first['data-time'].to_i) %></p>
    <p><%= tweet.xpath(".//p[@class='js-tweet-text tweet-text']").text %></p>
  </div>
<% } %>